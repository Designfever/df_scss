{
  "name": "projectName",
  "author": "author name",
  "copyright": "company",
  "version": "1.0.0",
  "description": "descptions...",
  "main": "server.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js --open",
    "gulp": "gulp"
  },
  "license": "ISC",
  "browserslist": [
    "last 2 version",
    "iOS >= 9"
  ],
  "paths": {
    "src": {
      "base": "./src/",
      "css": "./src/assets/css/",
      "fonts": "./src/assets/fonts/",
      "json": "./src/json/",
      "js": "./src/assets/js/",
      "img": "./src/assets/images/",
      "scss": "./src/assets/scss/",
      "favicon": "./src/favicon/"
    },
    "dist_mode": {
      "dev": "./dist/dev/",
      "product": "./dist/prd/"
    },
    "dist": {
      "base": "",
      "css": "assets/css/",
      "js": "assets/js/",
      "fonts": "assets/fonts/",
      "img": "assets/images/",
      "favicon": "favicon/"
    },
    "build": {
      "base": "./build/",
      "css": "./build/assets/css/",
      "js": "./build/assets/js/",
      "html": "./build/html/",
      "img": "./build/assets/images/"
    },
    "templates": "./src/templates/",
    "favicon": "/favicon"
  },
  "globs": {
    "vendorCss": [
      "./node_modules/bootstrap/dist/css/bootstrap.css"
    ],
    "distPrdCss": [
      "./dist/prd/assets/css/globals.css",
      "./dist/prd/assets/css/custom.css"
    ],
    "workScss": [
      "./src/assets/scss/globals.scss",
      "./src/assets/scss/custom.scss"
    ],
    "vendorJs": [
      "./node_modules/jquery/dist/jquery.js",
      "./node_modules/bootstrap/dist/js/bootstrap.bundle.js",
      "./src/assets/js/vendor/modernizr_dectectizr.js"
    ],
    "babelJs": [
      "./src/assets/js/interactive/*.js"
    ],
    "fonts": [
      "./src/assets/fonts/*.{eot,ttf,woff,woff2}"
    ],
    "html": [
      {
        "template": "./src/templates/template.base.html",
        "source": "./src/html/main.html",
        "target": ""
      },
      {
        "template": "./src/templates/template.base.html",
        "source": "./src/html/sub.html",
        "target": ""
      }
    ]
  },
  "urls": {
    "live": "http://live.sitename.com/",
    "dev": "http://dev.sitename.com/",
    "critical": "http://sitename.com/"
  },
  "varsFileNames": {
    "siteVendorsCssName": "projectName.vendors.css",
    "siteVendorsJsName": "projectName.vendors.js",
    "templateHtmlName": "template.base.html"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.4",
    "ejs": "^2.6.2",
    "express": "^4.16.4",
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.1.6",
    "@babel/traverse": "^7.4.5",
    "browser-sync": "^2.12.10",
    "chalk": "^2.4.1",
    "critical": "^1.3.4",
    "del": "^4.1.1",
    "fancy-log": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-css-replace-url": "^0.2.4",
    "gulp-cssmin": "^0.2.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-debug": "^4.0.0",
    "gulp-favicons": "^2.2.6",
    "gulp-filter": "^5.0.1",
    "gulp-header": "^2.0.5",
    "gulp-if": "^2.0.1",
    "gulp-image": "^4.3.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-inject": "^5.0.2",
    "gulp-livereload": "^4.0.1",
    "gulp-load-plugins": "^1.6.0",
    "gulp-newer": "^1.2.0",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-pretty-html": "^2.0.10",
    "gulp-print": "^5.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-streamify": "1.0.2",
    "gulp-template-html": "^0.2.2",
    "gulp-uglify": "^3.0.1",
    "moment": "^2.14.1",
    "prismjs": "^1.15.0",
    "through2": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "vinyl-named": "^1.1.0",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-stream": "^5.2.1"
  }
}
